<!doctype html>
<html>
	<head>

	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">

	<title>{{#resume.basics}}{{name}}{{/resume.basics}}</title>

	<style>
	{{{css}}}
	</style>
	<style media="print">
		body { font-size: 12px; }
	</style>

	<link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,300' rel='stylesheet' type='text/css'>

	</head>
	<body>

	<div id="resume">

	{{#resume.basics}}
	<section class="heading">
		<header>
			<h1>{{name}}</h1>
			{{#if label}}
			<h2 class="label">{{label}}</h2>
			{{/if}}
			<div class="contact">
				{{#if location}}
				<span class="detail location">
					{{location.city}}{{#if location.region}}, {{location.region}}{{/if}}
				</span>
				{{/if}}
				{{#if email}}
				<span class="detail">
					<a href="mailto:{{email}}">{{email}}</a>
				</span>
				{{/if}}
				{{#if phone}}
				<span class="detail">
					{{phone}}
				</span>
				{{/if}}
				{{#if website}}
				<span class="detail">
					<a href="{{website}}">{{website}}</a>
				</span>
				{{/if}}
				{{#if url}}
				{{#unless website}}
				<span class="detail">
					<a href="{{url}}">{{url}}</a>
				</span>
				{{/unless}}
				{{/if}}
				{{#if profiles.length}}
				{{#each profiles}}
				<span class="detail profile">
					{{network}}: <a href="{{url}}">{{username}}</a>
				</span>
				{{/each}}
				{{/if}}
			</div>
		</header>


		{{#if summary}}
		<p class="summary">{{paragraphSplit summary}}</p>
		{{/if}}

	</section>
	{{/resume.basics}}

	{{#if resume.work.length}}
	<section class="experience">
		<h2>Experience</h2>
		{{#each resume.work}}
		<section class="item">
			<header>
				{{#if name}}
					<h3 class="name">{{name}}</h3>
				{{/if}}
				<span class="dates">
					{{#if startDate}}
					<span class="startDate">
						{{date startDate}}
					</span>
					{{/if}}
					{{#if endDate}}
					<span class="endDate">
						- {{date endDate}}
					</span>
					{{else}}
					<span class="endDate">
						- Present
					</span>
					{{/if}}
				</span>
			</header>
			{{#if position}}
			<span class="position">
				{{position}}
			</span>
			{{/if}}

			{{#if summary}}
			<div class="summary">
				<p>{{paragraphSplit summary}}</p>
			</div>
			{{/if}}
			{{#if highlights.length}}
			<ul class="highlights">
				{{#each highlights}}
				<li>{{.}}</li>
				{{/each}}
			</ul>
			{{/if}}
		</section>
		{{/each}}
	</section>
	{{/if}}

	{{#if resume.earlierWork.length}}
	<div class="page-break"></div>
	<section class="earlier-experience">
		<div class="page-header">Nathan Storey</div>
		<h2>Earlier Experience</h2>
		{{#each resume.earlierWork}}
		<section class="item">
			<header>
				{{#if name}}
					<h3 class="name">{{name}}</h3>
				{{/if}}
				<span class="dates">
					{{#if startDate}}
					<span class="startDate">
						{{date startDate}}
					</span>
					{{/if}}
					{{#if endDate}}
					<span class="endDate">
						- {{date endDate}}
					</span>
					{{/if}}
				</span>
			</header>
			{{#if position}}
			<span class="position">
				{{position}}
			</span>
			{{/if}}

			{{#if summary}}
			<div class="summary">
				<p>{{summary}}</p>
			</div>
			{{/if}}
			{{#if highlights.length}}
			<ul class="highlights">
				{{#each highlights}}
				<li>{{.}}</li>
				{{/each}}
			</ul>
			{{/if}}
		</section>
		{{/each}}
	</section>
	{{/if}}

	{{#if resume.skills.length}}
	<section id="skills">
		<h2>Skills</h2>
		{{#each resume.skills}}
		<div class="item">
			{{#if name}}
			<div class="name">
				{{name}}
			</div>
			{{/if}}
			{{#if keywords.length}}
			<div class="keywords">
				{{#each keywords}}
					{{.}}{{#if @last}}{{else}},{{/if}}
				{{/each}}
			</div>
			{{/if}}
		</div>
		{{/each}}
	</section>
	{{/if}}

	{{#if resume.education.length}}
	<section id="education">
		<h2>Education</h2>
		{{#each resume.education}}
		<div class="item">
			<header>
				{{#if institution}}
					<h3 class="name">{{institution}}</h3>
				{{/if}}
			</header>

			<div class="qualification">
				{{#if studyType}}
				<span class="studyType">
					{{studyType}}
				</span>
				{{/if}}

				{{#if area}}
				<span class="area">
					{{#if studyType}}- {{/if}}{{area}}
				</span>
				{{/if}}

				{{#if startDate}}
				<span class="dates">
					({{date startDate}} - {{#if endDate}}{{date endDate}}{{else}}Present{{/if}})
				</span>
				{{else}}
				{{#if endDate}}
				<span class="endDate">
					({{date endDate}})
				</span>
				{{/if}}
				{{/if}}
			</div>

		</div>
		{{/each}}
	</section>
	{{/if}}

	{{#if resume.publications.length}}
	<section id="publications">
		<h2>Selected Talks & Civic Engagement</h2>
		{{#each resume.publications}}
		<div class="item">
			<span class="pub-name">{{name}}</span>
			{{#if publisher}}
			<span class="pub-publisher"> - {{publisher}}</span>
			{{/if}}
			{{#if releaseDate}}
			<span class="pub-date"> ({{releaseDate}})</span>
			{{/if}}
		</div>
		{{/each}}
	</section>
	{{/if}}

	{{#if resume.volunteer.length}}
	<section class="experience">
		<h2>Volunteering</h2>
		{{#each resume.volunteer}}
		<div class="item">
			<header>
				{{#if organization}}
					<h3 class="name">{{organization}}</h3>
				{{/if}}
			</header>
			{{#if position}}
			<span class="position">
				{{position}}
			</span>
			{{/if}}

			{{#if startDate}}
			<span class="startDate">
				{{date startDate}}
			</span>
			{{/if}}
			{{#if endDate}}
			<span class="endDate">
				- {{date endDate}}
			</span>
			{{else}}
			<span class="endDate">
				- Present
			</span>
			{{/if}}

			{{#if summary}}
			<div class="summary">
				<p>{{summary}}</p>
			</div>
			{{/if}}
			{{#if highlights.length}}
			<ul class="highlights">
				{{#each highlights}}
				<li>{{.}}</li>
				{{/each}}
			</ul>
			{{/if}}
		</div>
		{{/each}}
	</section>
	{{/if}}

	</div>

	</body>
</html>
